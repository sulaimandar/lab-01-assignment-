#include <iostream>
using namespace std;
int main() {
	int num1,num2,num3;//input from the user
	cout<<"enter three numbers"<<endl;
	cin>>num1>>num2>>num3;
	int largest;
	if (num1>=num2 && num1>=num3)
	largest =num1;
	else if (num2>=num1 && num2>=num3)
	largest =num2;
	else
 largest =num3;
 
 cout<<"the largest num is"<<largest<<endl;
//check if all are equal
cout <<"all the numbers are equal."<<endl;

int smallest;
if (num1<=num2 && num1<=num3)
smallest =num1;
if (num2<=num1 && num2<=num3)
smallest =num2;
else 
smallest=num3;
cout<<"the smallest number is:"<<smallest<<endl;
}
	
